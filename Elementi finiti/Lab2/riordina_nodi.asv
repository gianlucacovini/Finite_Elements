function E_n = riordina_nodi(p, t, I, B)
% I Ã¨ il bordo di Neumann, B il bordo di Dirichlet
    n = size(t, 1);
    E_n = [];
    for tr=1:n
        for node=tr
            node_neumann = intersect(node, I);
            if size(node_neumann, 2) == 2
                E_n = [E_n; node_neumann];
            end
        end
    end
end
